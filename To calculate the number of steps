//WAP to check the no. of steps
#include <iostream>
using namespace std;
int length(int x);
int main() {
    int n,count;
    cout<<"enter the number of steps"<<'\n';
    cin>>n;
    for(int i=0;i<=n;i++)
    {
        for(int j=0;j<=n;j+=2)
        {
            count++;
        }
        
    }
    cout<<count;
}
    
